<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
	xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
                  http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd"
	version="2.1">
	<access>FIELD</access>
	<entity class="com.fils.glucose.domain.patient.address.Address">
		<table name="ADDRESS" />
		<attributes>
			<embedded name="addressLine1">
				<attribute-override name="value">
					<column name="ADDRESS_LINE1" />
				</attribute-override>
			</embedded>
			<embedded name="addressLine2">
				<attribute-override name="value">
					<column name="ADDRESS_LINE2" />
				</attribute-override>
			</embedded>
			<embedded name="zipcode">
				<attribute-override name="value">
					<column name="ZIPCODE" />
				</attribute-override>
			</embedded>
			<embedded name="region">
				<attribute-override name="value">
					<column name="REGION" />
				</attribute-override>
			</embedded>
			<embedded name="ciry">
				<attribute-override name="value">
					<column name="CITY" />
				</attribute-override>
			</embedded>
			<embedded name="country">
				<attribute-override name="value">
					<column name="COUNTRY" />
				</attribute-override>
			</embedded>
		</attributes>
	</entity>
	<embeddable
		class="com.fils.glucose.domain.patient.address.AddressLine">
		<attributes>
			<basic name="value">
				<column name="ADDRESS_LINE_VALUE" />
			</basic>
		</attributes>
	</embeddable>
	<embeddable
		class="com.fils.glucose.domain.patient.address.ZipCode">
		<attributes>
			<basic name="value">
				<column name="ZIPCODE_VALUE" />
			</basic>
		</attributes>
	</embeddable>
	<embeddable
		class="com.fils.glucose.domain.patient.address.City">
		<attributes>
			<basic name="value">
				<column name="CITY_VALUE" />
			</basic>
		</attributes>
	</embeddable>
	<embeddable
		class="com.fils.glucose.domain.patient.address.Region">
		<attributes>
			<basic name="value">
				<column name="REGION_VALUE" />
			</basic>
		</attributes>
	</embeddable>
	<embeddable
		class="com.fils.glucose.domain.patient.address.Country">
		<attributes>
			<basic name="value">
				<column name="COUNTRY_VALUE" />
			</basic>
		</attributes>
	</embeddable>
</entity-mappings>
