			<id name="doctorId">
			</id>
			<basic name="mondayStart">
				<column name="monday_start" />
			</basic>
			<basic name="mondayEnd">
				<column name="monday_end" />
			</basic>
			<basic name="tuesdayStart">
				<column name="tuesday_start" />
			</basic>
			<basic name="tuesdayEnd">
				<column name="tuesday_end" />
			</basic>
			<basic name="wednesdayStart">
				<column name="wednesday_start" />
			</basic>
			<basic name="wednesdayEnd">
				<column name="wednesday_end" />
			</basic>
			<basic name="thursdayStart">
				<column name="thursday_start" />
			</basic>
			<basic name="thursdayEnd">
				<column name="thursday_end" />
			</basic>
			<basic name="fridayStart">
				<column name="friday_start" />
			</basic>
			<basic name="fridayEnd">
				<column name="friday_end" />
			</basic>
		</attributes>
	</entity>
</entity-mappings>
